{
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "files.exclude": {
        "**/__pycache__": true
    },
    "editor.lineNumbers": "relative",
    "terminal.integrated.defaultProfile.osx": "zsh",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "vim.insertModeKeyBindingsNonRecursive": [
        {
            "before": ["j", "k"],
            "after": ["<ESC>"]
        },
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["J"],
            "after": ["5", "j"]
        },
        {
            "before": ["K"],
            "after": ["5", "k"]
        },
        {
            "before": ["<Leader>", "j"],
            "after": ["J"]
        },
        {
            "before": ["<C-h>"],
            "after": ["<C-w>", "h"]
        },
        {
            "before": ["<C-j>"],
            "after": ["<C-w>", "j"]
        },
        {
            "before": ["<C-k>"],
            "after": ["<C-w>", "k"]
        },
        {
            "before": ["<C-l>"],
            "after": ["<C-w>", "l"]
        },
        {
            "before": ["<Leader>", "t", "t"],
            "commands": [":tabnew"]
        },
        {
            "before": ["<Leader>", "t", "n"],
            "commands": [":tabnext"]
        },
        {
            "before": ["<Leader>", "t", "p"],
            "commands": [":tabprev"]
        },
        {
            "before": ["<Leader>", "t", "o"],
            "commands": [":tabo"]
        },
        {
        "before": ["leader", "w"],
        "commands": [
            "workbench.action.files.save",
        ]
        }
      ],
    "vim.leader": "<Space>",
    "window.zoomLevel": 1
}
